{% extends 'base.html.twig' %}

{% block title %}Contact{% endblock %}

{% block body %}

    <div class="example-wrapper">
        <h1>Contact</h1>
        <h2>{{ contact.lastName }}</h2>
        <h2>{{ contact.firstName }}</h2>
        <h2><a href="mailto:{{ contact.email }}">{{ contact.email }}</a></h2>
        <h2>{{ contact.phone }}</h2>
        <h2>{{ contact.category }}</h2>
        <h2>{{ contact.note }}</h2>
    </div>
{% endblock %}
